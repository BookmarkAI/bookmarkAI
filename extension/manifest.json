{
  "name": "BookmarkAI",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ], 
  "icons": {
    "16": "extension_logo.png", 
    "48": "extension_logo.png", 
    "128": "extension_logo.png"
  }
}
